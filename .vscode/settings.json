{
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "workbench.sideBar.location": "left",
    "workbench.iconTheme": "vs-minimal",
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.lineHeight": 26,
    "editor.rulers": [
        150
    ],
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.bracketPairColorization.enabled": true,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.enablePreview": false,
    "workbench.settings.editor": "json",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    // "files.autoSave": "onFocusChange",
    "explorer.openEditors.visible": 1,
    "atomKeymap.promptV3Features": true,
    "javascript.validate.enable": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/android/.gradle": true,
        "**/android/build": true,
        "**/android/app/build": true,
        "**/ios/build": true,
        "**/ios/Pods": true,
        "**/coverage": true
    },
    "extensions.ignoreRecommendations": false,
    "eslint.alwaysShowStatus": true,
    // "eslint.packageManager": "yarn",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.tabSize": 2,
    "tailwindCSS.includeLanguages": {
        "javascriptreact": "javascript",
        "typescriptreact": "javascript"
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "clsx\\(([^)]*)\\)",
            "'([^']*)'"
        ]
    ],
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            150
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off",
        "files.insertFinalNewline": true
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDelete": false,
    "editor.snippetSuggestions": "top",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "bracketPairColorizer.depreciation-notice": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true,
        "suppressLineUncommittedWarning": true
    },
    "editor.guides.bracketPairs": "active",
    "workbench.colorTheme": "Ayu Mirage",
    // "workbench.colorCustomizations": {
    //     "activityBar.background": "#363635e0",
    //     "titleBar.activeBackground": "#292619",
    //     "titleBar.activeForeground": "#242523",
    //     "icon.foreground": "#23241D",
    //     "symbolIcon.colorForeground": "#421685",
    //     "icon.colorForeground":  "#FF0000",
    //     "activityBar.inactiveForeground": "#0F0505"
    // },
    "workbench.editorAssociations": {
        "*.ttf": "default"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "chrome"
}